\documentclass{article}

% Packages
\usepackage{tikz}
\usetikzlibrary{trees}

% Document
\begin{document}

\title{Simple Tree Diagram}
\author{Author Name}
\date{\today}
\maketitle

\section{Tree Diagram}

\begin{center}
\begin{tikzpicture}[
 level 1/.style={sibling distance=60mm},
 level 2/.style={sibling distance=30mm}
]

% Root
\node {Root}
 child {node {Child 1}
  child {node {Grandchild 1}}
  child {node {Grandchild 2}}
 }
 child {node {Child 2}
  child {node {Grandchild 3}}
  child {node {Grandchild 4}}
 };

\end{tikzpicture}
\end{center}

\end{document}
